@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlySummonersBook" as ReadOnlySummonersBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class SummonersBook
Class ModelManager
Class UserPrefs

Class UniquePlayerList
Class Player
Class Address
Class Email
Class Name
Class Phone
Class Tag

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

SummonersBook .up.|> ReadOnlySummonersBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlySummonersBook
ModelManager -left-> "1" SummonersBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

SummonersBook *--> "1" UniquePlayerList
UniquePlayerList --> "~* all" Player
Player *--> Name
Player *--> Phone
Player *--> Email
Player *--> Address
Player *--> "*" Tag

Player -[hidden]up--> I
UniquePlayerList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Player
@enduml
